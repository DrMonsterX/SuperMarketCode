    <script>
        layui.use(['form', 'table', 'element', 'layer'], function () {
            var table = layui.table, element = layui.element, layer = layui.layer, form = layui.form;
            var $ = layui.jquery;
            //初始化表格
            table.render({
                elem: '#info_table'
                , url: '/PRODUCT_ATTRIBUTE/getJson/' //数据接口
                , method: "post"
                , cellMinWidth: 120
                , cols: [[
                    { field: 'PRODUCT_ID', title: '商品编码', sort: true }
                    , { field: 'PRODUCT_NAME', title: '商品名', sort: true }
                    , { field: 'SUPPLIER_ID', title: '供货商ID', sort: true }
                    , { field: 'PURCHASE_PRICE', title: '进货价格', sort: true }
                    , { field: 'SELL_PRICE', title: '标准售价', sort: true }
                    , { field: 'EXP', title: '保质期', sort: true }
                    , { field: 'TOTAL', title: '商品总数', sort: true }
                    , { align: 'center', toolbar: '#staff-table-bar', title: '操作' }
                ]],
                data: [{
                    "PRODUCT_ID": "001",
                    "SUPPLIER_ID": "002",
                    "PRODUCT_NAME": "123",
                    "PURCHASE_PRICE": "100",
                    "SELL_PRICE": "120",
                    "EXP": "800",
                    "TOTAL": "12"
                }]
            });
            // 搜索
            $(document).on('click', '#info-btn-search', function () {
                //alert($("#search-type").val() + '/' + $("input[name='title']").val());
                table.render({
                    elem: '#info_table'
                    , url: '/PRODUCT_ATTRIBUTE/search01/' + $("#search-type").val() + '/' + $("input[name='title']").val() //数据接口
                    , method: "post"
                    , cellMinWidth: 120
                    , cols: [[
                        { field: 'PRODUCT_ID', title: '商品编码', sort: true }
                        , { field: 'PRODUCT_NAME', title: '商品名', sort: true }
                        , { field: 'SUPPLIER_ID', title: '供货商ID', sort: true }
                        , { field: 'PURCHASE_PRICE', title: '进货价格', sort: true }
                        , { field: 'SELL_PRICE', title: '标准售价', sort: true }
                        , { field: 'EXP', title: '保质期', sort: true }
                        , { field: 'TOTAL', title: '商品总数', sort: true }
                        , { align: 'center', toolbar: '#staff-table-bar', title: '操作' }
                    ]]
                });
            });
            // 高级搜索
            $(document).on('click', '#info-btn-Hsearch', function () {
                layer.open({
                    type: 1,
                    title: '高级搜索',
                    area: '400px',
                    offset: 'auto',
                    content: $('#info-record-model2').html()
                });
            });
            //添加
            $(document).on('click', '#info-btn-add', function () {
                layer.open({
                    type: 1,
                    title: '添加商品',
                    area: '400px',
                    offset: 'auto',
                    content: $('#info-record-model3').html()
                });
            });
            // 工具条点击事件
            table.on('tool(info_table)', function (obj) {
                var data = obj.data;
                if (obj.event === 'edit') { //修改
                    layer.open({
                        type: 1,
                        title: '商品信息修改',
                        area: '400px',
                        offset: 'auto',
                        content: $('#info-record-model').html(),
                        success: function () {
                            form.val('info-update-form', data);
                        }
                    });
                } else if (obj.event === 'del') { //删除
                    layer.confirm('确定要删除吗？', { title: '提示' }, function (index) {
                        $.post('/PRODUCT_ATTRIBUTE/Delete/' + data.PRODUCT_ID);
                        obj.del();
                        layer.close(index);
                    });
                }
            });
            //提交按钮监听
            //
            //高级搜索提交
            form.on('submit(advanced-search-submit)', function (data) {
                var elem1 = data.field.SUPPLIER_ID;
                var elem2 = data.field.PURCHASE_PRICE_MIN;
                var elem3 = data.field.PURCHASE_PRICE_MAX;
                var elem4 = data.field.SELL_PRICE_MIN;
                var elem5 = data.field.SELL_PRICE_MAX;
                var elem6 = data.field.EXP_MIN;
                var elem7 = data.field.EXP_MAX;
                var elem8 = data.field.TOTAL_MIN;
                var elem9 = data.field.TOTAL_MAX;

                if (elem1 == "") elem1 = "!!";
                if (elem2 == "") elem2 = "!!";
                if (elem3 == "") elem3 = "!!";
                if (elem4 == "") elem4 = "!!";
                if (elem5 == "") elem5 = "!!";
                if (elem6 == "") elem6 = "!!";
                if (elem7 == "") elem7 = "!!";
                if (elem8 == "") elem8 = "!!";
                if (elem9 == "") elem9 = "!!";

                alert('/PRODUCT_ATTRIBUTE/advancedSearch/' + elem1 + '/' + elem2 + '/' + elem3 + '/' + elem4 + '/' + elem5 + '/' + elem6 + '/' + elem7 + '/' + elem8 + '/' + elem9);
                //重载表格
                table.render({
                    elem: '#info_table'
                    , url: '/PRODUCT_ATTRIBUTE/advancedSearch/' + elem1 + '/' + elem2 + '/' + elem3 + '/' + elem4 + '/' + elem5 + '/' + elem6 + '/' + elem7 + '/' + elem8 + '/' + elem9 //数据接口
                    , method: "post"
                    , cellMinWidth: 120
